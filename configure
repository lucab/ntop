    echo "*    'make sntop' (or on *BSD and Solaris systems, 'gmake sntop')"
else
    echo "*    'make' (or on *BSD and Solaris systems, 'gmake')"
fi
echo "*"
echo "*******************************************************************"
echo

exit 0
