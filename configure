        # Expansion of NTOP_APPENDS(${GDOME_INCLUDE}, ${GDOME_LIB}, gdome)
    if test ".${GDOME_INCLUDE}" != "."; then
        rc=`(echo $CPPFLAGS | grep '${GDOME_INCLUDE} ' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
           CPPFLAGS="$CPPFLAGS -I${GDOME_INCLUDE}"
        fi
        rc=`(echo $INCS | grep '${GDOME_INCLUDE}' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
            INCS="$INCS -I${GDOME_INCLUDE}"
        fi
    fi
    if test ".${GDOME_LIB}" != "."; then
        rc=`(echo $LDFLAGS | grep '${GDOME_LIB} ' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
                LDFLAGS="$LDFLAGS -L${GDOME_LIB}"
        fi
    fi
    if test ".gdome" != "."; then
        rc=`(echo $LIBS | grep '\-lgdome ' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
            LIBS="$LIBS -lgdome"
        fi
    fi
# Finished expansion of NTOP_APPENDS()

        # Expansion of NTOP_APPENDS(${GLIB_INCLUDE}, ${GLIB_LIB}, glib)
    if test ".${GLIB_INCLUDE}" != "."; then
        rc=`(echo $CPPFLAGS | grep '${GLIB_INCLUDE} ' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
           CPPFLAGS="$CPPFLAGS -I${GLIB_INCLUDE}"
        fi
        rc=`(echo $INCS | grep '${GLIB_INCLUDE}' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
            INCS="$INCS -I${GLIB_INCLUDE}"
        fi
    fi
    if test ".${GLIB_LIB}" != "."; then
        rc=`(echo $LDFLAGS | grep '${GLIB_LIB} ' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
                LDFLAGS="$LDFLAGS -L${GLIB_LIB}"
        fi
    fi
    if test ".glib" != "."; then
        rc=`(echo $LIBS | grep '\-lglib ' > /dev/null 2> /dev/null; echo $?)`
        if [ $rc -eq 1 ]; then
            LIBS="$LIBS -lglib"
        fi
    fi
# Finished expansion of NTOP_APPENDS()

    fi
fi

